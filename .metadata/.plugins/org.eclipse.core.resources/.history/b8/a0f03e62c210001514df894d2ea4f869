import java.awt.Graphics2D;

public class PowerUp {
	private double x;
	private double y;
	private int r;

	private int type;
	
	
	public PowerUp(int type, double x, double y) {
		this.type = type;
		this.x = x;
		this.y = y;
	}
	
	public double getX() {
		return x;
	}
	
	public double getY() {
		return y;
	}
	
	public double getR() {
		return r;
	}
	
	public int getType() {
		return type;
	}
	
	public void update() {
		
	}
	
	public void draw(Graphics2D g) {
		
	}
	
}
